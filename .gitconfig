[alias]
  st = status
  cm = commit -v -m
  ad = add -A
  br = branch
  unstage = reset HEAD --
  last = log -1 HEAD
  files = "!f() { git diff --name-status $1^!; }; f"
[branch]
  #autosetuprebase = always         
[pull]
  #rebase = true
  rebase = false
[push]
	default = simple
